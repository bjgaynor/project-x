<%= link_to "Delete All Listings", :controller => "listings", :action => "destroy_all", :method => 'delete', :class => "delete_button" %>

  <br>
  <br>

<%= simple_table_for @listings do |t| %>
  <% t.column :id %>
  <% t.column :address %>
  <% t.column :city_state_zip %>
  <% t.column :zestimate %>
  <% t.column :rent_zestimate %>
  <% t.column :forecast_percentage %>
  <% t.actions do %>
    <% action {|listing| link_to 'Delete', listing, method: :delete } %>
  <% end %>
<% end %>